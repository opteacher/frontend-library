{
  "name": "frontend-library",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:e2e": "start-server-and-test preview :4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' :4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "test:com": "concurrently \"tailwindcss -i ./src/assets/main.css -o ./cypress/support/index.css --watch\" \"cypress open\" "
  },
  "dependencies": {
    "ant-design-vue": "^3.2.15",
    "dayjs": "^1.11.7",
    "happy-dom": "^8.9.0",
    "qs": "^6.11.0",
    "tiny-emitter": "^2.1.0",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "vue3-ace-editor": "^2.2.2",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.18.12/xlsx-0.18.12.tgz"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.12",
    "@types/qs": "^6.9.7",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vitest/coverage-c8": "^0.28.5",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "cypress": "^12.7.0",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "postcss": "^8.4.20",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "start-server-and-test": "^1.15.2",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "~4.7.4",
    "vite": "^4.0.0",
    "vite-plugin-istanbul": "^4.0.1",
    "vitest": "^0.25.6",
    "vue-tsc": "^1.0.12"
  }
}
