{
  "name": "frontend-library",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
    "test:e2e": "start-server-and-test preview :4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' :4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "test:com": "concurrently \"tailwindcss -i ./src/assets/main.css -o ./cypress/support/index.css --watch\" \"cypress open\" ",
    "test:com:all": "npx cypress run --component --record --key 9cdffdd4-b9e2-4817-bb89-a671ed8b988e"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "ant-design-vue": "^4.2.6",
    "codemirror": "^6.0.1",
    "codemirror-editor-vue3": "^2.8.0",
    "dayjs": "^1.11.13",
    "happy-dom": "^17.4.4",
    "paho-mqtt": "^1.1.0",
    "qs": "^6.13.0",
    "tiny-emitter": "^2.1.0",
    "uuid": "^11.1.0",
    "vanilla-jsoneditor": "^3.3.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue3-ace-editor": "^2.2.4",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.18.12/xlsx-0.18.12.tgz"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.0",
    "@cypress/vue": "^5.0.5",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@rushstack/eslint-patch": "^1.1.4",
    "@tailwindcss/postcss": "^4.1.3",
    "@types/codemirror": "^5.60.15",
    "@types/cypress__code-coverage": "^3.10.0",
    "@types/jsdom": "^20.0.1",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.9.0",
    "@types/paho-mqtt": "^1.0.10",
    "@types/qs": "^6.9.17",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vitest/coverage-c8": "^0.28.5",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^7.6.0",
    "cypress": "^12.7.0",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "postcss": "^8.5.3",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "start-server-and-test": "^1.15.2",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.1",
    "typescript": "~5.7.2",
