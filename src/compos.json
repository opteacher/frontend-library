{
  "ColorSelect": {
    "ctype": "String",
    "category": "field",
    "props": [
      {
        "label": "颜色",
        "refer": "color",
        "vtype": "String",
        "default": "#000000",
        "ftype": "Input",
        "placeholder": "输入颜色代码（16进制）",
        "vModel": true
      },
      {
        "label": "预设颜色",
        "refer": "preset",
        "vtype": "Array",
        "ftype": "EditList",
        "extra": {
          "lblProp": "color",
          "inline": true,
          "mapper": {
            "color": {
              "placeholder": "预设颜色",
              "type": "Input",
              "rules": [
                {
                  "required": true,
                  "message": "输入预设颜色"
                }
              ]
            }
          },
          "newFun": "return () => ({ color: '#000000' })"
        }
      }
    ]
  },
  "Checkbox": {
    "ctype": "Boolean",
    "category": "field",
    "props": [
      {
        "label": "选中标签",
        "refer": "chkLabels",
        "vtype": "Array",
        "default": "[]",
        "ftype": "EditList",
        "placeholder": "[0]: false; [1]:true",
        "extra": {
          "inline": true,
          "flatItem": true,
          "mapper": {
            "data": {
              "placeholder": "真假标签",
              "type": "Input",
              "rules": [
                {
                  "required": true,
                  "message": "输入真假标签"
                }
              ]
            }
          },
          "newFun": "return () => ({ data: '' })"
        }
      },
      {
        "label": "多选项",
        "refer": "options",
        "vtype": "Array",
        "ftype": "EditList",
        "extra": {
          "lblProp": "label",
          "inline": true,
          "mapper": {
            "label": {
              "placeholder": "显示标签",
              "type": "Input",
              "rules": [
                {
                  "required": true,
                  "message": "输入标签"
                }
              ]
            },
            "value": {
              "placeholder": "对应值",
              "type": "Input",
              "rules": [
                {
                  "required": true,
                  "message": "输入值"
                }
              ]
            }
          },
          "newFun": "return () => ({ label: '', value: '' })"
        }
      }
    ]
  },
  "EditList": {
    "ctype": "Array",
    "category": "field",
    "props": [
      {
        "label": "子表单结构",
        "refer": "mapper",
        "ftype": "JsonEditor",
        "vtype": "Object"
      },
      {
        "label": "捋平数组",
        "refer": "flatItem",
        "ftype": "Switch",
        "vtype": "Boolean"
      },
      {
        "label": "标签字段",
        "refer": "lblProp",
        "ftype": "Select",
        "vtype": "String",
        "placeholder": "选择标签字段"
      },
      {
        "label": "标签字典",
        "refer": "lblMapper",
        "ftype": "EditList",
        "vtype": "Array",
        "extra": {
          "lblProp": "label",
          "inline": false,
          "mapper": {
            "value": {
              "label": "",
              "type": "Input",
              "rules": [
                {
                  "required": true,
                  "message": "请输入关联字段！"
                }
              ],
              "desc": "",
              "placeholder": "输入关联字段",
              "disabled": false,
              "loading": false,
              "display": true,
              "reset": false,
              "empty": false,
              "onChange": "return () => undefined"
            },
            "label": {
              "label": "",
              "type": "Input",
              "rules": [],
              "desc": "",
              "placeholder": "输入标签",
              "disabled": false,
              "loading": false,
              "display": true,
              "reset": false,
              "empty": false,
              "onChange": "return () => undefined"
            }
          },
          "newFun": "return () => ({ refer: '', label: '', ftype: 'Input' })",
          "onChange": "return (state, array) => { state.field.mapper = createByFields(array) }"
        }
      }
    ]
  },
  "IconField": {
    "ctype": "String",
    "category": "field",
    "props": [
      {
        "label": "选中的图标",
        "refer": "icon",
        "vtype": "String",
        "ftype": "Input",
        "placeholder": "从ant图标库中选择",
        "vModel": true
      }
    ]
  },
  "UploadFile": {
    "ctype": "Array",
    "category": "field",
    "props": [
      {
        "label": "根表单",
        "refer": "form",
        "vtype": "Object",
        "ftype": "JsonEditor",
        "default": "{ \"files\": [] }"
      },
      {
        "label": "上传URL",
        "refer": "path",
        "vtype": "String",
        "ftype": "Input",
        "default": "/frontend-library/api/v1/asset/upload"
      },
      {
        "label": "请求头",
        "refer": "headers",
        "vtype": "Object",
        "ftype": "JsonEditor"
      },
      {
        "label": "文件列表",
        "refer": "value",
        "vtype": "Array",
        "ftype": "EditList",
        "extra": {
          "lblProp": "name",
          "inline": false,
          "mapper": {
            "name": {
              "label": "文件名",
              "type": "Input"
            },
            "status": {
              "label": "状态",
              "type": "Select",
              "options": [
                {
                  "label": "上传中",
                  "value": "uploading"
                },
                {
                  "label": "完成",
                  "value": "done"
                },
                {
                  "label": "出错",
                  "value": "error"
                },
                {
                  "label": "已移除",
                  "value": "removed"
                }
              ]
            }
          },
          "emitter": "return new Emitter()",
          "newFun": "return () => ({ name: '', status: 'removed' })"
        }
      },
      {
        "label": "上传前回调",
        "desc": "如果要中断上传，则返回false",
        "refer": "onBeforeUpload",
        "vtype": "Function",
        "ftype": "CodeEditor"
      },
      {
        "label": "上传中回调",
        "refer": "onChange",
        "vtype": "Function",
        "ftype": "CodeEditor"
      }
    ]
  },
  "CodeEditor": {
    "ctype": "String",
    "category": "field",
    "props": [
      {
        "label": "内容",
        "refer": "value",
        "vtype": "String",
        "ftype": "Textarea",
        "vModel": true
      },
      {
        "label": "语言",
        "refer": "lang",
        "vtype": "String",
        "ftype": "Select",
        "default": "javascript",
        "extra": {
          "options": [
            {
              "label": "javascript",
              "value": "javascript"
            },
            {
              "label": "json",
              "value": "json"
            }
          ]
        }
      }
    ]
  },
  "TagList": {
    "ctype": "Array",
    "category": "field",
    "components": [
      {
        "name": "FormItem",
        "ctype": "Unknown",
        "category": "field",
        "slot": "formItem",
        "props": [
          {
            "refer": "form",
            "default": "form"
          },
          {
            "refer": "skey",
            "default": "elKey"
          },
          {
            "refer": "mapper",
            "default": "value"
          },
          {
            "refer": "update:fprop",
            "vtype": "Function",
            "vOn": true,
            "default": "return values => console.log(values)"
          }
        ]
      }
    ],
    "props": [
      {
        "label": "值",
        "refer": "value",
        "vtype": "Array",
        "ftype": "EditList",
        "vModel": true,
        "extra": {
          "inline": true,
          "mapper": {
            "content": {
              "type": "Input"
            }
          }
        }
      },
      {
        "label": "标签字段",
        "refer": "lblProp",
        "vtype": "String",
        "ftype": "Select",
        "placeholder": "Object.keys(mapper)，如果抹平元素，则该选项禁用"
      },
      {
        "label": "抹平元素",
        "placeholder": "抹平单元素表单列表：[{ key: 'abc' }] => ['abc']，（注意：会抹去元素的键信息）",
        "refer": "flatItem",
        "vtype": "Boolean",
        "ftype": "Checkbox",
        "default": true
      },
      {
        "label": "标签映射",
        "refer": "lblDict",
        "vtype": "Object",
        "ftype": "JsonEditor",
        "expable": true,
        "extra": {
          "mainMenuBar": false,
          "navigationBar": false,
          "statusBar": false
        }
      },
      {
        "label": "表单信息",
        "refer": "mapper",
        "vtype": "Object",
        "ftype": "JsonEditor",
        "expable": true,
        "default": {
          "content": {
            "type": "Input"
          }
        },
        "extra": {
          "mainMenuBar": false,
          "navigationBar": false,
          "statusBar": false
        }
      },
      {
        "label": "新增函数",
        "refer": "newFun",
        "vtype": "Function",
        "ftype": "CodeEditor",
        "default": "return () => ({ content: '' })",
        "expable": true
      }
    ]
  },
